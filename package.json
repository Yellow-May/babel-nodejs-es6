{
	"name": "node-es6",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"babel:node": "babel-node",
		"dev": "nodemon --exec yarn babel:node -- src",
		"babel:build": "./node_modules/.bin/babel src --out-dir server",
		"server:clean": "rm -rf server",
		"start": "node server",
		"heroku-postbuild": "YARN_CONFIG_PRODUCTION=false yarn babel:build"
	},
	"dependencies": {
		"core-js": "^3.20.1",
		"express": "^4.17.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.5",
		"@babel/node": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"nodemon": "^2.0.15"
	},
	"engines": {
		"node": "14.x",
		"yarn": "1.x"
	}
}
